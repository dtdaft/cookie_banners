{
  "manifest_version": 2,

  "name": "Hide cookie banners",
  "version": "0.1.0",
  "description": "This extension hide cookie banners",

      "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Hide cookie banners"
  },


  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["content.js"],
    "matches": ["<all_urls>"]
  }]


    
}
